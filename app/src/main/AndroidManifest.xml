<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.aicte">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/aicte"
        android:label="@string/app_name"
        android:roundIcon="@drawable/aicte"
        android:supportsRtl="true"
        android:theme="@style/Theme.AICTE"
        tools:targetApi="31">
        <activity
            android:name=".Dashboard"
            android:exported="false" />
        <activity
            android:name=".StudenSignup"
            android:exported="false" />
        <activity
            android:name=".StudentLoginSignup"
            android:exported="false" />
        <activity
            android:name=".UniversityLogin"
            android:exported="false" />
        <activity
            android:name=".CollegeLogin"
            android:exported="false" />
        <activity
            android:name=".FacultyLogin"
            android:exported="false" />
        <activity
            android:name=".StudentLogin"
            android:exported="false" />
        <activity
            android:name=".Announcement"
            android:exported="false" />
        <activity
            android:name=".QuickLinks"
            android:exported="false" />
        <activity
            android:name=".DashboardScreen"
            android:exported="false" />
        <activity
            android:name=".About_us"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AICTE">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>